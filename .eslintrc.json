{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:vue/essential", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": ["warn", "tab"],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"],
        "vue/comment-directive": ["error", {
        "reportUnusedDisableDirectives": false
      }]
    }
    
}
